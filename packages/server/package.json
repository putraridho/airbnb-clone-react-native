  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "test": "NODE_ENV=test jest --watch",
    "build": "rimraf ./dist && tsc && copyfiles -u 1 src/**/*.graphql ./dist",
    "gen-schema-types": "ts-node src/scripts/createTypes.ts",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -c development -n"
  },
  "jest": {
    "rootDir": "./src",
    "testURL": "http://localhost/",
    "globalSetup": "./testUtils/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
